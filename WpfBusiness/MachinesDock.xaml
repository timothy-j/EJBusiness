<UserControl x:Class="MachinesDock"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:xcad="http://schemas.xceed.com/wpf/xaml/avalondock"
             xmlns:local="clr-namespace:WpfBusiness"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel Margin="3">
        <Button Content="HX" Margin="3" Command="local:MainWindow.OpenMachineRoutedCommand" CommandParameter="{Binding Content, RelativeSource={RelativeSource Self}}"/>
        <Button Content="R2" Margin="3" Command="local:MainWindow.OpenMachineRoutedCommand" CommandParameter="{Binding Content, RelativeSource={RelativeSource Self}}"/>
        <Button Content="HSF" Margin="3" Command="local:MainWindow.OpenMachineRoutedCommand" CommandParameter="{Binding Content, RelativeSource={RelativeSource Self}}"/>
        <Button Content="W3" Margin="3" Command="local:MainWindow.OpenMachineRoutedCommand" CommandParameter="{Binding Content, RelativeSource={RelativeSource Self}}"/>
        <Button Content="All" Margin="3" Command="local:MainWindow.OpenMachineRoutedCommand" CommandParameter="{Binding Content, RelativeSource={RelativeSource Self}}"/>
    </StackPanel>
</UserControl>
